<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kamedon.com.imageprocessingsample2019">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".page.photo.PhotoActivity"></activity>
        <activity android:name=".page.translate.TranslationActivity" />
        <activity android:name=".page.rotation.frame.FrameRotationActivity" />
        <activity android:name=".page.rotation.debug.FrameDebugActivity" />
        <activity android:name=".page.frame.FrameActivity" />
        <activity android:name=".page.frame.FrameUIActivity" />
        <activity android:name=".page.edit.EditActivity" />
        <activity android:name=".page.collision.square.CollisionSquareActivity" />
        <activity android:name=".page.collision.circle.CollisionCircleActivity" />
        <activity android:name=".page.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>